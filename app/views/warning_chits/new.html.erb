<h1>Warning Chit New</h1>

<%= form_for :letter_chit, url: game_warning_chits_path, html: { method: :post } do |f| %>
  <b>Game</b> <%= f.text_field :game_id, :value => "#{params[:game_id]}" %></br>
  <b>Name</b> <%= f.text_field :name %></br>
  <b>Tile ID</b> <%= f.text_field :tile_id %></br>
  <b>Letter</b> <%= f.text_field :letter %></br>
  <%= f.submit "New!" %>
<% end %>
